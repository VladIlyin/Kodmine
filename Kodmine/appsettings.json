{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=Kodmine;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "PostPageCount": 10,
  "PostKeysSeparator": "#",
  "HtmlCleanerRegEx": [
	["\\n", " "],
	["<div.*?>", ""],
	["<\\/div>", ""],
	["<span.*?>", ""],
	["<\\/span>", ""],
	["<code.*?>", ""],
	["<\\/code>", ""],
	["\\s+style=.*?>", ">"]
  ]
}